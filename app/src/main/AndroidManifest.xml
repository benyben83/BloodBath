<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dell_optilex_3010.bloodbath">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.FirstPage">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CreationPageOne" />
        <activity android:name=".activities.CreationPageTwo" />

        <service
            android:name=".services.MusicPlayer"
            android:enabled="true" />

        <activity android:name=".activities.CreationRecap" />
        <activity android:name=".activities.Arena" />
        <activity android:name=".activities.WinnerActivity" />
        <activity android:name=".activities.LooserActivity"></activity>
    </application>

</manifest>